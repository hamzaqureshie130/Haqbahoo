@{
    ViewData["Title"] = "About Us";
}
<!-- Page Header Start -->
<div class="page-header bg-section parallaxie">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <!-- Page Header Box Start -->
                <div class="page-header-box">
                    <h1 class="text-anime-style-3" data-cursor="-opaque">About Us</h1>
                    <nav class="wow fadeInUp">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a asp-controller="home" asp-area="" asp-action="index">home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">about us</li>
                        </ol>
                    </nav>
                </div>
                <!-- Page Header Box End -->
            </div>
        </div>
    </div>
</div>
<!-- Page Header End -->
<!-- Page About Us Section Start -->
<div class="about-us page-about-us">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6">
                <!-- About Us Image Start -->
                <div class="about-image">
                    <!-- About Image Start -->
                    <div class="about-img-1">
                        <figure class="reveal">
                            <img src="/assets/site/images/about-img-1.jpg" alt="">
                        </figure>
                    </div>
                    <!-- About Image End -->
                    <!-- About Image Start -->
                    <div class="about-img-2">
                        <figure class="reveal">
                            <img src="/assets/site/images/about-img-2.jpg" alt="">
                        </figure>
                    </div>
                    <!-- About Image End -->
                </div>
                <!-- About Us Image End -->
            </div>

            <div class="col-lg-6">
                <!-- About Us Content Start -->
                <div class="about-content">
                    <!-- Section Title Start -->
                    <div class="section-title">
                        <h3 class="wow fadeInUp">about us</h3>
                        <h2 class="text-anime-style-3" data-cursor="-opaque">Your trusted partner in reliable car rental</h2>
                        <p class="wow fadeInUp" data-wow-delay="0.25s">At Haqbahoo Rent a Car, we take pride in offering a hassle-free and comfortable rental experience. Whether you’re a tourist exploring the mesmerizing landscapes or a local in need of a reliable ride, our well-maintained fleet ensures a smooth journey.</p>
                    </div>
                    <!-- Section Title End -->
                    <!-- About Content Body Start -->
                    <div class="about-content-body">
                        <!-- About Trusted Booking Start -->
                        <div class="about-trusted-booking wow fadeInUp" data-wow-delay="0.5s">
                            <div class="icon-box">
                                <img src="/assets/site/images/icon-about-trusted-1.svg" alt="">
                            </div>
                            <div class="trusted-booking-content">
                                <h3>easy booking process</h3>
                                <p>We Have Optimized The Booking Process So That Our Clients Can Experience The Easiest And The Safest Service</p>
                            </div>
                        </div>
                        <!-- About Trusted Booking End -->
                        <!-- About Trusted Booking Start -->
                        <div class="about-trusted-booking wow fadeInUp" data-wow-delay="0.75s">
                            <div class="icon-box">
                                <img src="/assets/site/images/icon-about-trusted-2.svg" alt="">
                            </div>
                            <div class="trusted-booking-content">
                                <h3>convenient pick-up & return process</h3>
                                <p>We Have Optimized The Booking Process So That Our Clients Can Experience The Easiest And The Safest Service</p>
                            </div>
                        </div>
                        <!-- About Trusted Booking End -->
                    </div>
                    <!-- About Content Body End -->
                </div>
                <!-- About Us Content End -->
            </div>
        </div>
    </div>
</div>
<!-- Page About Us Section End -->
<!-- Vision Mission Section Start -->
<div class="vision-mission">
    <div class="container">
        <div class="row section-row">
            <div class="col-lg-12">
                <!-- Section Title Start -->
                <div class="section-title">
                    <h3 class="wow fadeInUp">Our Vision & Mission</h3>
                    <h2 class="text-anime-style-3" data-cursor="-opaque">Your Journey, Our Commitment</h2>
                </div>
                <!-- Section Title End -->
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <!-- Sidebar Our Vision Mission Nav start -->
                <div class="our-projects-nav wow fadeInUp" data-wow-delay="0.25s">
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item" role="presentation">
                            <button class="nav-link active" id="home-tab" data-bs-toggle="tab" data-bs-target="#vision" type="button" role="tab" aria-selected="true">Our Vision</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#mission" type="button" role="tab" aria-selected="false">Our Mission</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#approach" type="button" role="tab" aria-selected="false">Our Approach</button>
                        </li>
                    </ul>
                </div>
                <!-- Sidebar Our Vision Mission Nav End -->
                <!-- Vision Mission Box Start -->
                <div class="vision-mission-box tab-content" id="myTabContent">
                    <!-- Our Vision Item Start -->
                    <div class="our-vision-item tab-pane fade show active" id="vision" role="tabpanel">
                        <div class="row align-items-center">
                            <div class="col-lg-6">
                                <div class="vision-mission-content">
                                    <div class="section-title">
                                        <h3 class="wow fadeInUp">Our Vision</h3>
                                        <h2 class="text-anime-style-3" data-cursor="-opaque">Redefining Car Rental Experiences</h2>
                                        <p class="wow fadeInUp" data-wow-delay="0.25s">To become the leading car rental service provider, delivering unparalleled customer satisfaction by offering well-maintained vehicles, competitive pricing, and a seamless rental experience.</p>
                                    </div>
                                    <div class="vision-mission-list wow fadeInUp" data-wow-delay="0.5s">
                                        <ul>
                                            <li>Provide reliable and well-maintained vehicles</li>
                                            <li>Offer seamless online booking and support</li>
                                            <li>Ensure safety and comfort for every journey</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="vision-image">
                                    <figure class="image-anime reveal">
                                        <img src="/assets/site/images/our-vision-img.jpg" alt="Our Vision">
                                    </figure>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Our Vision Item End -->
                    <!-- Our Mission Item Start -->
                    <div class="our-vision-item tab-pane fade" id="mission" role="tabpanel">
                        <div class="row align-items-center">
                            <div class="col-lg-6">
                                <div class="vision-mission-content">
                                    <div class="section-title">
                                        <h3>Our Mission</h3>
                                        <h2 data-cursor="-opaque">Driving Trust and Reliability</h2>
                                        <p>To offer a diverse fleet of high-quality vehicles while ensuring transparent pricing, exceptional customer service, and a hassle-free rental process tailored to meet every traveler's needs.</p>
                                    </div>
                                    <div class="vision-mission-list">
                                        <ul>
                                            <li>Deliver prompt and professional service</li>
                                            <li>Maintain transparency in pricing and policies</li>
                                            <li>Continuously improve our services through customer feedback</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="vision-image">
                                    <figure class="image-anime reveal">
                                        <img src="/assets/site/images/our-mission-img.jpg" alt="Our Mission">
                                    </figure>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Our Mission Item End -->
                    <!-- Our Approach Item Start -->
                    <div class="our-mission-item tab-pane fade" id="approach" role="tabpanel">
                        <div class="row align-items-center">
                            <div class="col-lg-6">
                                <div class="vision-mission-content">
                                    <div class="section-title">
                                        <h3>Our Approach</h3>
                                        <h2 data-cursor="-opaque">Customer-Centric Service</h2>
                                        <p>We believe in making car rentals convenient and accessible by embracing technology, valuing customer feedback, and continuously enhancing our service standards to exceed expectations.</p>
                                    </div>
                                    <div class="vision-mission-list">
                                        <ul>
                                            <li>Personalized services for every client</li>
                                            <li>Embrace technology for smooth operations</li>
                                            <li>Commitment to sustainability and community</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="vision-image">
                                    <figure class="image-anime reveal">
                                        <img src="/assets/site/images/our-approach-img.jpg" alt="Our Approach">
                                    </figure>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Our Approach Item End -->
                </div>
                <!-- Vision Mission Box End -->
            </div>
        </div>
    </div>
</div>
<!-- Vision Mission Section End -->
<!-- Why Choose Us Section Start -->
<div class="why-choose-us">
    <div class="container">
        <div class="row section-row">
            <div class="col-lg-12">
                <!-- Section Title Start -->
                <div class="section-title">
                    <h3 class="wow fadeInUp">why choose us</h3>
                    <h2 class="text-anime-style-3" data-cursor="-opaque">We Understand Your Travel Needs Instantly</h2>
                </div>
                <!-- Section Title End -->
            </div>
        </div>

        <div class="row align-items-center">
            <div class="col-lg-4 col-md-6 order-lg-1 order-md-1 order-1">
                <!-- Why Choose Item Start -->
                <div class="why-choose-item wow fadeInUp">
                    <div class="icon-box">
                        <img src="/assets/site/images/icon-why-choose-1.svg" alt="">
                    </div>
                    <div class="why-choose-content">
                        <h3>Wide Range of Vehicles</h3>
                        <p>From economy cars to luxury SUVs, we offer a diverse fleet to suit your travel needs in Islamabad.</p>
                    </div>
                </div>
                <!-- Why Choose Item End -->
                <!-- Why Choose Item Start -->
                <div class="why-choose-item wow fadeInUp" data-wow-delay="0.25s">
                    <div class="icon-box">
                        <img src="/assets/site/images/icon-why-choose-2.svg" alt="">
                    </div>
                    <div class="why-choose-content">
                        <h3>24/7 Customer Support</h3>
                        <p>Our team is always available to assist you with bookings, inquiries, or any on-road support.</p>
                    </div>
                </div>
                <!-- Why Choose Item End -->
            </div>

            <div class="col-lg-4 col-md-12 order-lg-2 order-md-3 order-2">
                <div class="why-choose-image">
                    <figure class="reveal">
                        <img src="/assets/site/images/why-choose-img.jpg" alt="">
                    </figure>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 order-lg-3 order-md-2 order-3">
                <!-- Why Choose Item Start -->
                <div class="why-choose-item wow fadeInUp">
                    <div class="icon-box">
                        <img src="/assets/site/images/icon-why-choose-3.svg" alt="">
                    </div>
                    <div class="why-choose-content">
                        <h3>Easy & Hassle-Free Booking</h3>
                        <p>Book your car in just a few clicks—simple, quick, and convenient!</p>
                    </div>
                </div>
                <!-- Why Choose Item End -->
                <!-- Why Choose Item Start -->
                <div class="why-choose-item wow fadeInUp" data-wow-delay="0.25s">
                    <div class="icon-box">
                        <img src="/assets/site/images/icon-why-choose-4.svg" alt="">
                    </div>
                    <div class="why-choose-content">
                        <h3>Flexible Rental Plans</h3>
                        <p>Choose from daily, weekly, or long-term rental options, customized to fit your schedule.</p>
                    </div>
                </div>
                <!-- Why Choose Item End -->
            </div>
        </div>
    </div>
</div>
<!-- Why Choose Us Section End -->

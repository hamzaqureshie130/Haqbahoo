                        @* <p>Enhance your rental experience with additional options.</p> *@
                    </div>
                    <div class="service-footer">
                        <a asp-controller="workshop" asp-action="detail" asp-route-id="@service.Id" class="section-icon-btn"><img src="/assets/site/images/arrow-white.svg" alt=""></a>

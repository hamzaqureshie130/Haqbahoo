@using DomainLayer.Haqbahoo.Entities
@model WorkShop

<!-- Page Header Start -->
<div class="page-header bg-section parallaxie">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <!-- Page Header Box Start -->
                <div class="page-header-box">
                    <h1 class="text-anime-style-3" data-cursor="-opaque">@Model.Title</h1>
                    <nav class="wow fadeInUp">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a asp-controller="home" asp-area="" asp-action="index">home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">workshop detail</li>
                            @* <li class="breadcrumb-item active" aria-current="page">business car rental</li> *@
                        </ol>
                    </nav>
                </div>
                <!-- Page Header Box End -->
            </div>
        </div>
    </div>
</div>
<!-- Page Header End -->
<!-- Page Service Single Start -->
<div class="page-service-single">
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <!-- Service Sidebar Start -->
                <div class="service-sidebar">
                    <!-- Service Categories List Start -->
                   @*  <div class="service-catagery-list wow fadeInUp">
                        <h3>our services</h3>
                        <ul>
                            <li><a href="#">Car Rental with Driver</a></li>
                            <li><a href="#">Business Car Rental</a></li>
                            <li><a href="#">Airport Transfer</a></li>
                            <li><a href="#">chauffeur services</a></li>
                        </ul>
                    </div> *@
                    <!-- Service Categories List End -->
                    <!-- Sidebar Cta Start -->
                    <div class="sidebar-cta-box wow fadeInUp" data-wow-delay="0.25s">
                        <!-- Cta Contact Item Start -->
                        <div class="cta-contact-item">
                            <div class="icon-box">
                                <img src="images/icon-sidebar-cta.svg" alt="">
                            </div>
                            <div class="cta-contact-content">
                                <h2>Need Help?</h2>
                                <p>We strive to provide exceptional customer service and support. Whether you have questions about our services or need assistance, we’re here to help.</p>
                                <p><strong>Contact Us:</strong></p>
                                <p>📞 <a href="tel:+923245708356">+92 324 5708356</a></p>
                                <p>📧 <a href="mailto:info@haqbahoo.co">info@haqbahoo.co</a></p>
                            </div>

                            <div class="cta-contact-btn">
                                <a href="tel:+923245708356" class="btn-default">Call Now</a>
                            </div>

                        </div>
                        <!-- Cta Contact Item End -->
                    </div>
                    <!-- Sidebar Cta End -->
                </div>
                <!-- Service Sidebar End -->
            </div>

            <div class="col-lg-8">
                <!-- Service Single Content Start -->
                <div class="service-single-content">
                    <!-- Service Featured Image Start -->
                    <div class="service-featured-image">
                        <figure class="image-anime reveal">
                            <img src="/@Model.CoverImageUrl" alt="">
                        </figure>
                    </div>
                    <!-- Service Featured Image End -->
                    <!-- Service Entry Content Start -->
                    <div class="service-entry">
                        <h2 class="text-anime-style-3" data-cursor="-opaque">@Model.Title</h2>
                        @* <p class="wow fadeInUp" data-wow-delay="0.2s">We offer a wide range of car rental services designed to meet all your transportation needs. Whether you’re traveling for business, planning a family vacation, or need a reliable vehicle for a special event, we have the perfect solution for you.</p> *@
                        <p class="wow fadeInUp" data-wow-delay="0.4s">@Model.Description</p>
                       @*  <ul class="wow fadeInUp" data-wow-delay="0.6s">
                            <li>24/7 Roadside Assistance</li>
                            <li>Free Cancellation & Return</li>
                            <li>Rent Now Pay When You Arrive</li>
                            <li>24/7 Roadside Assistance</li>
                            <li>Free Cancellation & Return</li>
                            <li>Rent Now Pay When You Arrive</li>
                        </ul> *@

                        <!-- Service Entry Gallery Start -->
                        <div class="service-entry-gallery">
                            <div class="row gallery-items">
                                <div class="col-lg-3 col-6">
                                    <!-- image gallery start -->
                                    <div class="service-gallery wow fadeInUp" data-wow-delay="0.2s" data-cursor-text="View">
                                        <a href="images/service-gallery-1.jpg">
                                            <figure class="image-anime">
                                                <img src="/assets/site/siteimages/1.jpg" alt="img">
												
                                            </figure>
                                        </a>
                                    </div>
                                    <!-- image gallery end -->
                                </div>

                                <div class="col-lg-3 col-6">
                                    <!-- image gallery start -->
                                    <div class="service-gallery wow fadeInUp" data-wow-delay="0.4s" data-cursor-text="View">
                                        <a href="images/service-gallery-2.jpg">
                                            <figure class="image-anime">
                                                <img src="/assets/site/siteimages/2.jpg" alt="img">
                                            </figure>
                                        </a>
                                    </div>
                                    <!-- image gallery end -->
                                </div>

                                <div class="col-lg-3 col-6">
                                    <!-- image gallery start -->
                                    <div class="service-gallery wow fadeInUp" data-wow-delay="0.6s" data-cursor-text="View">
                                        <a href="images/service-gallery-3.jpg">
                                            <figure class="image-anime">
                                                <img src="/assets/site/siteimages/3.jpg" alt="img">
                                            </figure>
                                        </a>
                                    </div>
                                    <!-- image gallery end -->
                                </div>

                                <div class="col-lg-3 col-6">
                                    <!-- image gallery start -->
                                    <div class="service-gallery wow fadeInUp" data-wow-delay="0.8s" data-cursor-text="View">
                                        <a href="images/service-gallery-4.jpg">
                                            <figure class="image-anime">
                                                <img src="/assets/site/siteimages/4.jpg" alt="img">
                                            </figure>
                                        </a>
                                    </div>
                                    <!-- image gallery end -->
                                </div>
                            </div>
                        </div>
                        <!-- Service Entry Gallery End -->
                    </div>
                    <!-- Service Entry Content End -->
                    <!-- Rental Conditions Faqs Start -->
                    <div class="rental-conditions-faqs">
                        <!-- Section Title Start -->
                        <div class="section-title">
                            <h3 class="wow fadeInUp">frequently asked questions</h3>
                            <h2 class="text-anime-style-3" data-cursor="-opaque">You need to know about service</h2>
                        </div>
                        <!-- Section Title End -->
                        <!-- Workshop FAQ Accordion Start -->
                        <div class="rental-condition-accordion" id="rentalaccordion">
                            <!-- FAQ Item Start -->
                            <div class="accordion-item wow fadeInUp">
                                <h2 class="accordion-header" id="rentalheading1">
                                    <button class="accordion-button" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#rentalcollapse1" aria-expanded="true" aria-controls="rentalcollapse1">
                                        Are the mechanics at Haqbahoo certified and experienced?
                                    </button>
                                </h2>
                                <div id="rentalcollapse1" class="accordion-collapse collapse show" aria-labelledby="rentalheading1"
                                     data-bs-parent="#rentalaccordion">
                                    <div class="accordion-body">
                                        <p>Yes, all our mechanics are certified professionals with years of experience in car servicing and repairs.</p>
                                    </div>
                                </div>
                            </div>
                            <!-- FAQ Item End -->
                            <!-- FAQ Item Start -->
                            <div class="accordion-item wow fadeInUp" data-wow-delay="0.25s">
                                <h2 class="accordion-header" id="rentalheading2">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#rentalcollapse2" aria-expanded="false" aria-controls="rentalcollapse2">
                                        Do you use genuine parts and products for car services?
                                    </button>
                                </h2>
                                <div id="rentalcollapse2" class="accordion-collapse collapse" aria-labelledby="rentalheading2"
                                     data-bs-parent="#rentalaccordion">
                                    <div class="accordion-body">
                                        <p>We only use genuine parts and high-quality products to ensure the best performance and reliability for your car.</p>
                                    </div>
                                </div>
                            </div>
                            <!-- FAQ Item End -->
                            <!-- FAQ Item Start -->
                            <div class="accordion-item wow fadeInUp" data-wow-delay="0.5s">
                                <h2 class="accordion-header" id="rentalheading3">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#rentalcollapse3" aria-expanded="false" aria-controls="rentalcollapse3">
                                        How long does a typical service take?
                                    </button>
                                </h2>
                                <div id="rentalcollapse3" class="accordion-collapse collapse" aria-labelledby="rentalheading3"
                                     data-bs-parent="#rentalaccordion">
                                    <div class="accordion-body">
                                        <p>The duration of the service depends on the type of work required. A basic service usually takes around 1-2 hours.</p>
                                    </div>
                                </div>
                            </div>
                            <!-- FAQ Item End -->
                            <!-- FAQ Item Start -->
                            <div class="accordion-item wow fadeInUp" data-wow-delay="0.75s">
                                <h2 class="accordion-header" id="rentalheading4">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#rentalcollapse4" aria-expanded="false" aria-controls="rentalcollapse4">
                                        Can I watch my car being serviced?
                                    </button>
                                </h2>
                                <div id="rentalcollapse4" class="accordion-collapse collapse" aria-labelledby="rentalheading4"
                                     data-bs-parent="#rentalaccordion">
                                    <div class="accordion-body">
                                        <p>Yes, we have a dedicated viewing area where you can watch the service process.</p>
                                    </div>
                                </div>
                            </div>
                            <!-- FAQ Item End -->
                            <!-- FAQ Item Start -->
                            <div class="accordion-item wow fadeInUp" data-wow-delay="1s">
                                <h2 class="accordion-header" id="rentalheading5">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#rentalcollapse5" aria-expanded="false" aria-controls="rentalcollapse5">
                                        Is there any warranty on the services provided?
                                    </button>
                                </h2>
                                <div id="rentalcollapse5" class="accordion-collapse collapse" aria-labelledby="rentalheading5"
                                     data-bs-parent="#rentalaccordion">
                                    <div class="accordion-body">
                                        <p>Yes, we offer a warranty on all our services to ensure customer satisfaction and trust in our work.</p>
                                    </div>
                                </div>
                            </div>
                            <!-- FAQ Item End -->
                        </div>
                        <!-- Workshop FAQ Accordion End -->

                    </div>
                    <!-- Rental Conditions Faqs End -->
                </div>
                <!-- Service Single Content End -->
            </div>
        </div>
    </div>
</div>
